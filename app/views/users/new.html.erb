<%= form_for User.new, :url => {:action => "add_user"} do |user| %>
 <%= user.label :email %>
 <br>
 <%=user.email_field :email%>
 <br >
  <%= user.label :password %>
  <br >
  <%=user.password_field :password%>
  <br >
  <%= user.label :confirm_password %>
  <br >
  <%=user.password_field :password_confirmation%>
  <br >
  <%=user.submit 'create'%>
<%end%>

<%= simple_form_for User.new, url: {action: "add_user"} do |u| %>
<%= u.input :email %>
<%= u.input :password %>
<%= u.input :password_confirmation %>
<%= u.button :submit %>
<% end %>
